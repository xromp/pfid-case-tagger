{
  "manifest_version": 2,
  "name": "DPS Github Reporter",
  "description": "This sample shows how to authorize Firebase in a Chrome extension using a Google account.",
  "version": "2.1",
  "icons": {
    "128": "firebase-logo.png"
  },
  "browser_action": {
    "default_icon": "firebase-logo.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/firebase/*/issues/*"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "web_accessible_resources": [
    "templates/*",
    "libs/*"
  ],
  "permissions": [
    "identity",
    "https://sheets.googleapis.com/*",
    "https://docs.google.com/spreadsheets/"
  ],
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'",
  "oauth2": {
    "client_id": "147018354613-7totq6mjp5378s9a9nuvdr1jr37qa0af.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile",
      "https://www.googleapis.com/auth/spreadsheets"

    ]
  },
  "key": ""
}